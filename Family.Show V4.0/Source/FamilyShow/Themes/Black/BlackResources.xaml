        <StackPanel ToolTipService.InitialShowDelay="300">
            <StackPanel.ToolTip>
                <ToolTip Style="{DynamicResource DiagramToolTip}" Content="{Binding Path=BottomLabel}" />
            </StackPanel.ToolTip>

            <StackPanel Margin="0,0,0,5">
                <Border BorderBrush="{Binding Path=NodePhotoStroke}" Background="#02000000" HorizontalAlignment="Center" VerticalAlignment="Top" BorderThickness="1.5" x:Name="AvatarPhotoBorder">
                    <Border.BitmapEffect>
                        <DropShadowBitmapEffect ShadowDepth="2" Softness="0.3"/>
                    </Border.BitmapEffect>
                    <Image Name="Person" Source="{Binding Path=Person.Avatar, Converter={StaticResource ImageConverter}, Mode=OneWay}" Stretch="Uniform" VerticalAlignment="Top" MaxWidth="88" MaxHeight="56" HorizontalAlignment="Stretch" StretchDirection="Both" />
                </Border>
            </StackPanel>

            <Label Content="{Binding Path=BottomLabel}" Padding="0,0,0,0" HorizontalAlignment="Center" TextBlock.TextAlignment="Right" Foreground="{DynamicResource FontColor}" FontSize="9"></Label>

        </StackPanel>
    </ControlTemplate>


